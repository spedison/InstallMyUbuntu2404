[a11y]

[a11y/applications]
screen-keyboard-enabled=false
screen-magnifier-enabled=false

[a11y/keyboard]
mousekeys-enable=true

[app-folders]

[app-folders/folders/Pardus]

[app-folders/folders/Utilities]
apps=['gnome-abrt.desktop', 'gnome-system-log.desktop', 'nm-connection-editor.desktop', 'org.gnome.baobab.desktop', 'org.gnome.Connections.desktop', 'org.gnome.DejaDup.desktop', 'org.gnome.Dictionary.desktop', 'org.gnome.DiskUtility.desktop', 'org.gnome.Evince.desktop', 'org.gnome.FileRoller.desktop', 'org.gnome.fonts.desktop', 'org.gnome.Loupe.desktop', 'org.gnome.seahorse.Application.desktop', 'org.gnome.tweaks.desktop', 'org.gnome.Usage.desktop', 'vinagre.desktop']
categories=['X-GNOME-Utilities']
name='X-GNOME-Utilities.directory'
translate=true

[app-folders/folders/YaST]

[background]
color-shading-type='solid'
picture-options='zoom'
picture-uri='file:///home/spedison/.local/share/backgrounds/2024-08-30-07-42-52-20240331_155447.jpg'
picture-uri-dark='file:///home/spedison/.local/share/backgrounds/2024-08-30-07-42-52-20240331_155447.jpg'
primary-color='#000000000000'
secondary-color='#000000000000'

[input-sources]

[interface]
color-scheme='prefer-dark'
cursor-size=200
cursor-theme='DMZ-Black'
enable-hot-corners=true
gtk-theme='Yaru-dark'
icon-theme='Yaru'
locate-pointer=true
text-scaling-factor=1.29
toolkit-accessibility=false

[notifications]
application-children=['org-gnome-terminal', 'jetbrains-idea', 'google-chrome', 'gnome-power-panel', 'jetbrains-clion', 'apport-gtk', 'org-gnome-characters', 'xmind-xmind', 'org-gnome-dejadup', 'jetbrains-pycharm', 'io-snapcraft-sessionagent', 'org-gnome-eog', 'update-manager', 'snapd-desktop-integration-snapd-desktop-integration', 'virt-manager', 'vivaldi-vivaldi-stable', 'ideametit', 'org-gnome-nautilus', 'gnome-printers-panel', 'org-gnome-totem', 'transmission-gtk']
show-banners=true
show-in-lock-screen=true

[peripherals/keyboard]
numlock-state=true

[peripherals/mouse]
double-click=400

[privacy]
old-files-age=uint32 30
recent-files-max-age=-1

[remote-desktop/rdp]
enable=false
tls-cert='/home/spedison/.local/share/gnome-remote-desktop/certificates/rdp-tls.crt'
tls-key='/home/spedison/.local/share/gnome-remote-desktop/certificates/rdp-tls.key'
view-only=true

[screensaver]
color-shading-type='solid'
picture-options='zoom'
picture-uri='file:///home/spedison/.local/share/backgrounds/2024-08-30-07-42-52-20240331_155447.jpg'
primary-color='#000000000000'
secondary-color='#000000000000'

[search-providers]
sort-order=['org.gnome.Contacts.desktop', 'org.gnome.Documents.desktop', 'org.gnome.Nautilus.desktop']

[session]
idle-delay=uint32 0

[sound]
allow-volume-above-100-percent=true
event-sounds=true

[wm/keybindings]
maximize=@as []
unmaximize=@as []

[wm/preferences]
auto-raise=true
focus-mode='mouse'
num-workspaces=7
resize-with-right-button=false
